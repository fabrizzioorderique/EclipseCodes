import java.util.*;
import javax.swing.JOptionPane;
import SnakeGame.Entity; //class from another package within the project = packageName.className   
import MathPacket.Gcf;  //In order to use a class from another project, you have to put the project in the java path dependency
public class TestRunner {
	public static boolean first(int a, int b, int c) {
		if(a==0 || b*b -4*a*c <=0)
			return false;
		else
			return true;
	}
	public static boolean second(int a, int b, int c) {
		return a== 0 && b*b - 4*a*c >0;
	}
	public static void main(String[] args) {
		int a = 0,b=3,c=5;
		System.out.println(first(a,b,c));
		System.out.println(second(a,b,c));
	}
}
